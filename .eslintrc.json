{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "plugins": ["import"],
  "rules": {
    "import/named": "error",
    "import/namespace": "error",
    "import/default": "error",
    "import/export": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
